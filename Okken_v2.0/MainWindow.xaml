                                Grid.Row="1">
                                <TextBlock TextWrapping="Wrap" Text="{Binding}" TextAlignment="Center" VerticalAlignment="Center" FontWeight="Bold" FontSize="12">
                                Секция 1
                                </TextBlock>
                            </Border>

                            <Border x:Name="Sect2VSDBorder"
                                Style="{StaticResource HeaderBorderStyle2}"
                                Grid.Row="1">
                                <TextBlock TextWrapping="Wrap" Text="{Binding}" TextAlignment="Center" VerticalAlignment="Center" FontWeight="Bold" FontSize="12">
                                Секция 2
                                </TextBlock>
                            </Border>

                            <Border x:Name="VSDBorder"
                                Style="{StaticResource HeaderBorderStyle}"
                                Grid.Row="0">
                                <TextBlock TextWrapping="Wrap" Text="{Binding}" TextAlignment="Center" VerticalAlignment="Center" FontWeight="Bold" FontSize="12">
                                Частотный преобразователь VSD
                                </TextBlock>
                            </Border>

                            <Border x:Name="PFCBorder"
                                Style="{StaticResource HeaderBorderStyle}"
