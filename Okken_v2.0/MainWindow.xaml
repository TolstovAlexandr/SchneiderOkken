                                <ColumnDefinition Width="{Binding ElementName=Sect2MCC3, Path=ActualWidth, Mode=OneWay}"/>
                                <ColumnDefinition Width="{Binding ElementName=Sect2MCC4, Path=ActualWidth, Mode=OneWay}"/>
                                <ColumnDefinition Width="{Binding ElementName=Sect2MCC5, Path=ActualWidth, Mode=OneWay}"/>
                                <ColumnDefinition Width="{Binding ElementName=Sect2MCC6, Path=ActualWidth, Mode=OneWay}"/>
                                <ColumnDefinition Width="{Binding ElementName=Sect2MCC7, Path=ActualWidth, Mode=OneWay}"/>
                                <ColumnDefinition Width="{Binding ElementName=Sect2MCC8, Path=ActualWidth, Mode=OneWay}"/>
                                <ColumnDefinition Width="{Binding ElementName=Sect2MCC_Last, Path=ActualWidth, Mode=OneWay}"/>

                                <ColumnDefinition Width="{Binding ElementName=Sect1SS1, Path=ActualWidth, Mode=OneWay}"/>
                                <ColumnDefinition Width="{Binding ElementName=Sect1SS2, Path=ActualWidth, Mode=OneWay}"/>
                                <ColumnDefinition Width="{Binding ElementName=Sect1SS3, Path=ActualWidth, Mode=OneWay}"/>
                                <ColumnDefinition Width="{Binding ElementName=Sect1SS4, Path=ActualWidth, Mode=OneWay}"/>
                                <ColumnDefinition Width="{Binding ElementName=Sect1SS5, Path=ActualWidth, Mode=OneWay}"/>
                                <ColumnDefinition Width="{Binding ElementName=Sect1SS_Last, Path=ActualWidth, Mode=OneWay}"/>

                                <ColumnDefinition Width="{Binding ElementName=Sect2SS1, Path=ActualWidth, Mode=OneWay}"/>
                                <ColumnDefinition Width="{Binding ElementName=Sect2SS2, Path=ActualWidth, Mode=OneWay}"/>
                                <ColumnDefinition Width="{Binding ElementName=Sect2SS3, Path=ActualWidth, Mode=OneWay}"/>
                                <ColumnDefinition Width="{Binding ElementName=Sect2SS4, Path=ActualWidth, Mode=OneWay}"/>
                                <ColumnDefinition Width="{Binding ElementName=Sect2SS5, Path=ActualWidth, Mode=OneWay}"/>
                                <ColumnDefinition Width="{Binding ElementName=Sect2SS_Last, Path=ActualWidth, Mode=OneWay}"/>

                                <ColumnDefinition Width="{Binding ElementName=Sect1VSD1, Path=ActualWidth, Mode=OneWay}"/>
                                <ColumnDefinition Width="{Binding ElementName=Sect1VSD2, Path=ActualWidth, Mode=OneWay}"/>
                                <ColumnDefinition Width="{Binding ElementName=Sect1VSD3, Path=ActualWidth, Mode=OneWay}"/>
                                <ColumnDefinition Width="{Binding ElementName=Sect1VSD4, Path=ActualWidth, Mode=OneWay}"/>
                                <ColumnDefinition Width="{Binding ElementName=Sect1VSD5, Path=ActualWidth, Mode=OneWay}"/>
                                <ColumnDefinition Width="{Binding ElementName=Sect1VSD_Last, Path=ActualWidth, Mode=OneWay}"/>

                                <ColumnDefinition Width="{Binding ElementName=Sect2VSD1, Path=ActualWidth, Mode=OneWay}"/>
                                <ColumnDefinition Width="{Binding ElementName=Sect2VSD2, Path=ActualWidth, Mode=OneWay}"/>
                                <ColumnDefinition Width="{Binding ElementName=Sect2VSD3, Path=ActualWidth, Mode=OneWay}"/>
                                <ColumnDefinition Width="{Binding ElementName=Sect2VSD4, Path=ActualWidth, Mode=OneWay}"/>
                                <ColumnDefinition Width="{Binding ElementName=Sect2VSD5, Path=ActualWidth, Mode=OneWay}"/>
                                <ColumnDefinition Width="{Binding ElementName=Sect2VSD_Last, Path=ActualWidth, Mode=OneWay}"/>

                                <ColumnDefinition Width="{Binding ElementName=Sect1CompPowerComboBox, Path=ActualWidth, Mode=OneWay}"/>
                                <ColumnDefinition Width="{Binding ElementName=Sect2CompPowerComboBox, Path=ActualWidth, Mode=OneWay}"/>

                                <ColumnDefinition></ColumnDefinition>
                            </Grid.ColumnDefinitions>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="25"></RowDefinition>
                                <RowDefinition Height="40"></RowDefinition>
                            </Grid.RowDefinitions>

                            <!--Бордеры для надзаголовка-->
                            <Border x:Name="IncomersBorder"
                            Style="{StaticResource HeaderBorderStyle}"
                                Grid.Row="1">
                                <TextBlock TextWrapping="Wrap" Text="{Binding}" TextAlignment="Center" VerticalAlignment="Center" FontWeight="Bold" FontSize="12">
                                Вводные аппараты
                                </TextBlock>
                            </Border>

                            <Border x:Name="BTBorder"                          
                                Style="{StaticResource HeaderBorderStyle}"
                                Grid.Row="1">
                                <TextBlock TextWrapping="Wrap" Text="{Binding}" TextAlignment="Center" VerticalAlignment="Center"  FontWeight="Bold" FontSize="12">
                                Секционный аппарат
                                </TextBlock>
                            </Border>

                            <Border x:Name="Sec1FidersBorder"
                                Style="{StaticResource HeaderBorderStyle1}"
                                Grid.Row="1">
                                <TextBlock TextWrapping="Wrap" Text="{Binding}" TextAlignment="Center" VerticalAlignment="Center" FontWeight="Bold" FontSize="12">
                                Секция 1
                                </TextBlock>
                            </Border>

                            <Border x:Name="Sec2FidersBorder"
                                Style="{StaticResource HeaderBorderStyle2}"
                                Grid.Row="1">
                                <TextBlock TextWrapping="Wrap" Text="{Binding}" TextAlignment="Center" VerticalAlignment="Center" FontWeight="Bold" FontSize="12">
                                Секция 2
                                </TextBlock>
                            </Border>

                            <Border x:Name="FiderBorder"
                                Style="{StaticResource HeaderBorderStyle}"
                                Grid.Row="0">
                                <TextBlock TextWrapping="Wrap" Text="{Binding}" TextAlignment="Center" VerticalAlignment="Center" FontWeight="Bold" FontSize="12">
                                Отходящие фидеры DF
                                </TextBlock>
                            </Border>

                            <Border x:Name="Sec1MCCBorder"
                                Style="{StaticResource HeaderBorderStyle1}"
                                Grid.Row="1">
                                <TextBlock TextWrapping="Wrap" Text="{Binding}" TextAlignment="Center" VerticalAlignment="Center" FontWeight="Bold" FontSize="12">
                                Секция 1
                                </TextBlock>
                            </Border>

                            <Border x:Name="Sec2MCCBorder"
                                Style="{StaticResource HeaderBorderStyle2}"
                                Grid.Row="1">
                                <TextBlock TextWrapping="Wrap" Text="{Binding}" TextAlignment="Center" VerticalAlignment="Center" FontWeight="Bold" FontSize="12">
                                Секция 2
                                </TextBlock>
                            </Border>

                            <Border x:Name="MCCBorder"
                                Style="{StaticResource HeaderBorderStyle}"
                                Grid.Row="0">
                                <TextBlock TextWrapping="Wrap" Text="{Binding}" TextAlignment="Center" VerticalAlignment="Center" FontWeight="Bold" FontSize="12">
                                Управление двигателями MCC
                                </TextBlock>
                            </Border>

                            <Border x:Name="Sect1SSBorder"
                                Style="{StaticResource HeaderBorderStyle1}"
                                Grid.Row="1">
                                <TextBlock TextWrapping="Wrap" Text="{Binding}" TextAlignment="Center" VerticalAlignment="Center" FontWeight="Bold" FontSize="12">
                                Секция 1
                                </TextBlock>
                            </Border>

