                        </Grid>
                    </StackPanel>

                    <DataGrid x:Name="MainDataGrid"
                              AutoGenerateColumns="False" HorizontalGridLinesBrush="DarkGray"
                              RowBackground="LightGray" AlternatingRowBackground="White"
                              ml:ExcelActions.EnabledCopyExcel="True"
                              ml:ExcelActions.EnabledPasteExcel="True"
                              ml:ExcelActions.CreateContextMenu="True"
                              ml:ExcelActions.ContainsHeader="False"
                              ml:ExcelActions.CancelWithErrors="True"
                              ml:ExcelActions.PaintFlash="True"
                              ml:ExcelActions.ColorFlash="Yellow"
                              ml:ExcelActions.ShowErrorMessages="True"
                              CanUserAddRows="False"
                              CurrentCellChanged="MainDataGrid_CurrentCellChanged"
                              FrozenColumnCount="3"
                              HorizontalScrollBarVisibility="Auto"
                              ScrollViewer.ScrollChanged="MainDataGrid_ScrollChanged"        
                              MaxHeight="550">

                        <DataGrid.Resources>
                            <!--Стиль для выбранного столбца-->
                            <Style TargetType="DataGridCell" x:Key="SelectedColumnStyle">
                                <!--<Setter Property="Background" Value="#0079d8"/>-->
                                <Setter Property="Background" Value="Red"/>
                            </Style>

                            <!--Стиль для вводных аппаратов секции 1-->
                            <Style TargetType="DataGridCell" x:Key="IncomerColumnStyle1">
                                <Setter Property="Background" Value="LightSteelBlue"/>
                            </Style>

                            <!--Стиль для вводных аппаратов секции 2-->
                            <Style TargetType="DataGridCell" x:Key="IncomerColumnStyle2">
                                <Setter Property="Background" Value="MediumAquamarine"/>
                            </Style>

                            <Style TargetType="ScrollBar">
                                <Style.Triggers>
                                    <Trigger Property="Orientation" Value="Horizontal">
                                        <Setter Property="Height" Value="22"/>
                                    </Trigger>
                                    <Trigger Property="Orientation" Value="Vertical">
                                        <Setter Property="Width" Value="22"/>
                                    </Trigger>
                                </Style.Triggers>
                            </Style>

                        </DataGrid.Resources>

                        <DataGrid.ColumnHeaderStyle>
                            <Style TargetType="{x:Type DataGridColumnHeader}">
                                <Setter Property="FontWeight"  Value="Bold" />
                            </Style>
                        </DataGrid.ColumnHeaderStyle>

                        <DataGrid.Items>

                        </DataGrid.Items>

                        <DataGrid.Columns>
                            <DataGridTemplateColumn x:Name="MinusBtnCol" 
                                                Width="25"
                                                CanUserResize="False">
                                <DataGridTemplateColumn.CellTemplate>
                                    <DataTemplate>
                                        <Button x:Name="DeletRow" Content="-" Tag="{Binding}" Command="Delete"/>
                                    </DataTemplate>
                                </DataGridTemplateColumn.CellTemplate>
                            </DataGridTemplateColumn>

                            <DataGridTextColumn 
                                        x:Name="NoColText"
                                        Header="№"
                                        HeaderStyle="{StaticResource ColumnHeaderStyle}"
                                        Binding="{Binding Path=Id, Mode=OneWay}" 
                                        Width="40"/>

                            <DataGridTextColumn x:Name="NameOfSheetText"
                                        Header="Наименование щита"
                                        HeaderStyle="{StaticResource ColumnHeaderStyle}"
                                        Binding="{Binding Path=Name, Mode=TwoWay}" 
                                        Width="120"/>

                            <DataGridComboBoxColumn x:Name="NumsOfPoleComboBox"
                                            Width="70"
                                            Header="Кол-во полюсов"
                                            ElementStyle="{StaticResource ComboBoxStyle}"
                                            HeaderStyle="{StaticResource ColumnHeaderStyle}"
