                                <ColumnDefinition Width="{Binding ElementName=Sect1Fider13Text, Path=ActualWidth, Mode=OneWay}"/>
                                <ColumnDefinition Width="{Binding ElementName=Sect1Fider14Text, Path=ActualWidth, Mode=OneWay}"/>
                                <ColumnDefinition Width="{Binding ElementName=Sect1FiderLastText, Path=ActualWidth, Mode=OneWay}"/>

