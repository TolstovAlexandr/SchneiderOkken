                                <ColumnDefinition Width="{Binding ElementName=Sect1MCC1, Path=ActualWidth, Mode=OneWay}"/>
                                <ColumnDefinition Width="{Binding ElementName=Sect1MCC2, Path=ActualWidth, Mode=OneWay}"/>
                                <ColumnDefinition Width="{Binding ElementName=Sect1MCC3, Path=ActualWidth, Mode=OneWay}"/>
                                <ColumnDefinition Width="{Binding ElementName=Sect1MCC4, Path=ActualWidth, Mode=OneWay}"/>
                                <ColumnDefinition Width="{Binding ElementName=Sect1MCC5, Path=ActualWidth, Mode=OneWay}"/>
                                <ColumnDefinition Width="{Binding ElementName=Sect1MCC6, Path=ActualWidth, Mode=OneWay}"/>
