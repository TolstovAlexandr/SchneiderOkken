<Window x:Class="Okken.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Okken"
        mc:Ignorable="d"
        Title="MainWindow" Height="800" Width="1500"
        xmlns:ml="clr-namespace:MoralesLarios.Utilities.Excel;assembly=MoralesLarios.Utilities">
    <Window.Resources>
        <Style x:Key="ColumnHeaderStyle" TargetType="DataGridColumnHeader">
            <Setter Property="ContentTemplate">
                <Setter.Value>
                    <DataTemplate>
                        <Border BorderBrush="Black" BorderThickness="1" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Padding="3">
                            <TextBlock TextWrapping="Wrap" TextAlignment="Center" VerticalAlignment="Center" Text="{Binding}">
                                <TextBlock.LayoutTransform>
                                    <RotateTransform Angle="360" />
                                </TextBlock.LayoutTransform>
                            </TextBlock>
                        </Border>
                    </DataTemplate>
                </Setter.Value>
            </Setter>
            <Setter Property="FontWeight" Value="Bold"></Setter>
            <Setter Property="FontSize" Value="12"></Setter>
            <Setter Property="HorizontalContentAlignment" Value="Stretch" />
            <Setter Property="VerticalContentAlignment" Value="Stretch" />
            <Setter Property="Background" Value="LightGreen"></Setter>
            <Setter Property="Padding" Value="0"></Setter>
            <Style.Triggers>
                <Trigger Property="IsMouseOver" Value="True">
                    <Setter Property="ToolTip" Value="Нажмите для сортировки."/>
                    <Setter Property="Background" Value="LightBlue"/>
                </Trigger>
            </Style.Triggers>
        </Style>

        <Style x:Key="ColumnHeaderStyle2" 
               TargetType="DataGridColumnHeader"
               BasedOn="{StaticResource ColumnHeaderStyle}">
            <Setter Property="Background" Value="LightYellow"></Setter>
        </Style>

        <Style x:Key="ColumnHeaderStyle3" 
               TargetType="DataGridColumnHeader"
               BasedOn="{StaticResource ColumnHeaderStyle}">
            <Setter Property="Background" Value="LightSteelBlue"></Setter>

            <Setter Property="ContentTemplate">
                <Setter.Value>
                    <DataTemplate>
                        <Border BorderBrush="Black" BorderThickness="1" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Padding="3">
                            <TextBlock Text="{Binding}">
                                <TextBlock.LayoutTransform>
                                    <RotateTransform Angle="270" />
                                </TextBlock.LayoutTransform>
                            </TextBlock>
                        </Border>
                    </DataTemplate>
                </Setter.Value>
            </Setter>
        </Style>

        <Style x:Key="ColumnHeaderStyle4" 
               TargetType="DataGridColumnHeader"
               BasedOn="{StaticResource ColumnHeaderStyle3}">
            <Setter Property="Background" Value="MediumAquamarine"></Setter>
        </Style>

        <Style x:Key="ColumnHeaderStyle5" 
               TargetType="DataGridColumnHeader"
               BasedOn="{StaticResource ColumnHeaderStyle}">
            <Setter Property="Background" Value="MediumTurquoise"></Setter>
        </Style>

        <!--Стиль ComboBox для Выбора АВР-->
        <Style x:Key="ATSComboBoxStyle" 
               TargetType="DataGridColumnHeader"
               BasedOn="{StaticResource ColumnHeaderStyle}">
            <Setter Property="Background" Value="SandyBrown"></Setter>
            <Setter Property="ContentTemplate">
                <Setter.Value>
                    <DataTemplate>
                        <Border BorderBrush="Black" BorderThickness="1" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Padding="3">
                            <TextBlock Text="{Binding}">
                                <TextBlock.LayoutTransform>
                                    <RotateTransform Angle="270" />
                                </TextBlock.LayoutTransform>
                            </TextBlock>
                        </Border>
                    </DataTemplate>
                </Setter.Value>
            </Setter>
        </Style>

        <Style x:Key="HeaderBorderStyle" TargetType="Border">
            <Setter Property="BorderBrush" Value="Black"></Setter>
            <Setter Property="BorderThickness" Value="1"></Setter>
            <Setter Property="HorizontalAlignment" Value="Stretch"></Setter>
            <Setter Property="VerticalAlignment" Value="Stretch"></Setter>
            <Setter Property="Padding" Value="3"></Setter>
            <Setter Property="Background" Value="LightYellow"></Setter>
        </Style>

        <Style x:Key="HeaderBorderStyle1" TargetType="Border" BasedOn="{StaticResource HeaderBorderStyle}">
            <Setter Property="Background" Value="LightSteelBlue"></Setter>
        </Style>

        <Style x:Key="HeaderBorderStyle2" TargetType="Border" BasedOn="{StaticResource HeaderBorderStyle}">
            <Setter Property="Background" Value="MediumAquamarine"></Setter>
        </Style>

        <Style x:Key="HeaderBorderStyle3" TargetType="Border" BasedOn="{StaticResource HeaderBorderStyle}">
            <Setter Property="Background" Value="MediumTurquoise"></Setter>
        </Style>

        <Style x:Key="ComboBoxStyle" TargetType="ComboBox">
            <Setter Property="HorizontalContentAlignment" Value="Center"></Setter>
        </Style>

        <Style x:Key="TextCenterStyle" TargetType="TextBlock">
            <Setter Property="HorizontalAlignment" Value="Center"></Setter>
        </Style>

    </Window.Resources>
    <Grid>
        <StackPanel Orientation="Vertical">
            <!--Меню-->
            <!--*****************************************************************************************************************-->
            <Menu IsMainMenu="True">
                <MenuItem Header="File">
                    <MenuItem Header="New">
                        <MenuItem Header="Project..." InputGestureText="Ctrl+Shift+N"
                                  Click="New_Click"/>
                    </MenuItem>

                    <MenuItem Header="Open">
                        <MenuItem Header="Project..."
                                  Click="Open_Click"/>
                    </MenuItem>

                    <MenuItem Header="Save Project">
                        <MenuItem Header="Save" InputGestureText="Ctrl+S"
                                  Click="Save_Click"/>
                        <MenuItem Header="Save As"
                                  Click="SaveAs_Click"/>
                    </MenuItem>

                </MenuItem>
            </Menu>
            <!--*****************************************************************************************************************-->

            <DockPanel LastChildFill="False">
                <ToolBarTray DockPanel.Dock="Top">
                    <ToolBar Band="1" BandIndex="1">
                        <Button x:Name="NewFileButton" Height="30"
                                Click="New_Click">
                            <Image Source="/Icons/NewFile.png" />
                        </Button>
                        <Button x:Name="OpenButton" Height="30"
                                Click="Open_Click">
                            <Image Source="/Icons/Open.png" />
                        </Button>
                        <Button x:Name="SaveButton" Height="30"
                                Click="Save_Click">
                            <Image Source="/Icons/Save.png" />
                        </Button>
                        <Button x:Name="SaveAsButton" Height="30"
                                Click="SaveAs_Click">
                            <Image Source="/Icons/SaveAs.png" />
                        </Button>
                    </ToolBar>
                </ToolBarTray>
            </DockPanel>

            <GroupBox Header="OKKEN"
                  Margin="10"
                  Padding="10">
                <StackPanel>
                    <StackPanel Orientation="Horizontal">

                        <!--Бордеры для заголовка для выравнивания привязаны к ширене первых трех столбцов-->
